{
    "kernel-version" : "5.10",
    "initrd-is-root-used" : false,
    "confirmation-before-running" : true,
    "nproc" : 2,
    "qemu-option" : {
        "semp" : false,
        "samp" : false,
        "kaslr" : false,
        "cores" : 1,
        "threads" : 1
    },
    "ctf" : {
        "use-ctf-vmlinux" : true,
        "vmlinux-path" : "./tmp/vmlinux"
    }
}